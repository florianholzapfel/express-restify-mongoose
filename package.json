    "test:restify": "mocha -R spec ./test/restify.js --timeout 10s",
    "test:unit": "mocha -R spec ./test/unit.js",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "dot-prop": "^7.2.0",
    "lodash.isplainobject": "~4.0.6",
