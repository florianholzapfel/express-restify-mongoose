    "request-promise": "^1.0.2",
    "restify": "~3",
    "sinon": "~1",
    "standard": "^5.0.0"
  },
  "engines": {
    "node": ">=0.10"
  },
  "homepage": "http://florianholzapfel.github.io/express-restify-mongoose/",
  "keywords": [
    "ReST",
