    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "body-parser": "^1.19.0",
    "esbuild": "^0.15.12",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.25.0",
    "eslint-plugin-mocha": "^10.1.0",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "mocha": "^10.1.0",
    "prettier": "^2.7.1",
